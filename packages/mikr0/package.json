{
  "name": "mikr0",
  "type": "module",
  "version": "0.0.1-beta.23",
  "description": "Micro-frontend framework",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "mikr0": "dist/cli.js"
  },
  "scripts": {
    "lint": "biome check --write --unsafe .",
    "build": "tsc && npm run build:ui",
    "build:ui": "cd ui && npm run build",
    "test": "node --experimental-strip-types --test './src/**/*.test.*'",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "check-exports": "attw --pack . --ignore-rules=cjs-resolves-to-esm"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./dev": {
      "import": "./dist/dev/index.js",
      "types": "./dist/dev/index.d.ts"
    }
  },
  "keywords": [],
  "author": "",
  "files": [
    "README.md",
    "dist",
    "src"
  ],
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.676.0",
    "@aws-sdk/node-http-handler": "^3.370.0",
    "@azure/identity": "^4.4.1",
    "@azure/storage-blob": "^12.25.0",
    "@fastify/basic-auth": "^6.0.1",
    "@fastify/cors": "^10.0.1",
    "@fastify/express": "^4.0.1",
    "@fastify/multipart": "^9.0.0",
    "@fastify/static": "^8.0.1",
    "@fastify/type-provider-typebox": "^5.0.0",
    "@sinclair/typebox": "^0.33.12",
    "adm-zip": "^0.5.16",
    "esbuild": "^0.24.0",
    "fastify": "^5.0.0",
    "form-data": "^4.0.0",
    "knex": "^3.1.0",
    "lru-cache": "^11.0.1",
    "magicast": "^0.3.5",
    "node-dir": "^0.1.17",
    "prompts": "^2.4.2",
    "semver": "^7.6.3",
    "superjson": "^2.2.1",
    "undici": "^6.19.8",
    "vite": "^5.4.5"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.16.4",
    "@types/adm-zip": "^0.5.5",
    "@types/knex": "^0.15.2",
    "@types/node": "^22.5.5",
    "@types/node-dir": "^0.0.37",
    "@types/semver": "^7.5.8",
    "typescript": "^5.6.2"
  }
}
