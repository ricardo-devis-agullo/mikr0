{
  "name": "mikr0",
  "type": "module",
  "version": "0.1.13",
  "description": "Micro-frontend framework",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "mikr0": "dist/cli.js"
  },
  "scripts": {
    "lint": "biome check --write --unsafe .",
    "build": "tsc && npm run build:ui",
    "build:ui": "cd ui && npm run build",
    "test": "node --experimental-strip-types --test './test/**/*.test.*'",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "check-exports": "attw --pack . --ignore-rules=cjs-resolves-to-esm"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./dev": {
      "types": "./dist/dev/index.d.ts",
      "import": "./dist/dev/index.js",
      "require": "./dist/dev/index.js"
    },
    "./package.json": "./package.json"
  },
  "keywords": [],
  "author": "",
  "files": [
    "README.md",
    "dist",
    "src"
  ],
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.774.0",
    "@aws-sdk/node-http-handler": "^3.370.0",
    "@azure/identity": "^4.8.0",
    "@azure/storage-blob": "^12.27.0",
    "@fastify/basic-auth": "6.0.1",
    "@fastify/cors": "^10.0.1",
    "@fastify/middie": "^9.0.3",
    "@fastify/multipart": "9.0.0",
    "@fastify/static": "8.0.3",
    "@fastify/type-provider-typebox": "5.1.0",
    "@rdevis/turbo-stream": "^2.4.1",
    "@sinclair/typebox": "^0.34.31",
    "adm-zip": "^0.5.16",
    "chalk": "^5.4.1",
    "esbuild": "^0.25.1",
    "fastify": "5.0.0",
    "form-data": "^4.0.2",
    "knex": "^3.1.0",
    "lru-cache": "^11.1.0",
    "magicast": "^0.3.5",
    "memfs": "^4.17.0",
    "node-dir": "^0.1.17",
    "prompts": "^2.4.2",
    "semver": "^7.7.1",
    "sqlite3": "^5.1.7",
    "undici": "^7.5.0",
    "vite": "^6.2.3",
    "vite-plugin-css-injected-by-js": "^3.5.2"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.17.4",
    "@types/adm-zip": "^0.5.7",
    "@types/knex": "^0.16.1",
    "@types/node": "^22.13.13",
    "@types/node-dir": "^0.0.37",
    "@types/semver": "^7.5.8",
    "typescript": "^5.8.2"
  }
}